{
  "name": "UDL_frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently --kill-others \"npm run pack\" \"npm run sass\"",
    "pack": "webpack-dev-server --env dev",
    "sass": "node-sass scss/app.scss css/app.css --watch",
    "build": "node-sass scss/app.scss css/app.css --output-style compressed \n webpack -p --env build"
  },
  "dependencies": {
    "S": "0.0.1",
    "bootstrap-calendar": "^0.2.4",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.0.10",
    "jarallax": "^1.8.0",
    "jquery": "^3.2.1",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "react-intl": "^2.3.0",
    "tether": "^1.4.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "concurrently": "^3.5.0",
    "handlebars-loader": "^1.5.0",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.1"
  }
}
